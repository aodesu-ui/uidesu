    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "default": "./dist/schema/index.js"
    }
  },
  "bin": "./dist/index.js",
  "scripts": {
    "dev": "tsup --watch",
    "start:dev": "cross-env REGISTRY_URL=https://localhost:4000/r node dist/index.js"
  },
  "dependencies": {
    "@antfu/ni": "^25.0.0",
    "@dotenvx/dotenvx": "^1.48.4",
    "commander": "^14.0.0",
    "cosmiconfig": "^9.0.0",
    "deepmerge": "^4.3.1",
    "execa": "^9.6.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.1",
    "https-proxy-agent": "^7.0.6",
    "kleur": "^4.1.5",
    "node-fetch": "^3.3.2",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "3.24.6"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/fs-extra": "^11.0.4",
    "@types/prompts": "^2.4.9",
    "@types/stringify-object": "^4.0.5",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "type-fest": "^4.41.0",
    "typescript": "^5.9.2"
  }
}
